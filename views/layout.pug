  body
    block content
    #modalPromoteMessage.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')
          .modal-body
          .modal-footer
            button.btn.btn-primary(type='button', data-dismiss='modal') OK
    #modalAlertMessage.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')
          .modal-body
          .modal-footer
            button.btn.btn-secondary(type='button', data-dismiss='modal', id='btnCancelTransferCommand') NO
            button.btn.btn-primary(type='button', id='btnTransferCommand' , data-dismiss='modal' ) YES
    #modalCloseIncident.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')
          .modal-body
          .modal-footer
            button.btn.btn-secondary(type='button', data-dismiss='modal', id='btnCancelCloseIncident') NO
            button.btn.btn-primary(type='button', id='btnCloseIncident' , data-dismiss='modal' ) YES

    #modalSelectVehicle.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title Which car do you want to use during current shift?
          .modal-body
            .form-group
              label.form-control-label(for='vehicle-selector') Vehicles:
              select#vehicle-selector.custo-select.form-control(name="vehicles")
                option(selected="selected", value='None') None
          .modal-footer
            button#btnBoard.btn.btn-primary(type='button', data-dismiss='modal') Board!
    #modalNewSituationConfirmation.modal.fade(tabindex='-1', role='dialog', aria-labelledby='modalNewSituationConfirmation',
    aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title Caution!
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')
          .modal-body
            p Are you sure this situation does not already exist?
          .modal-footer
            button.btn.btn-secondary(type='button', data-dismiss='modal', id='cancelNewSituationBtn') NO
            button.btn.btn-primary(type='button', data-dismiss='modal', id='confirmNewSituationBtn') YES
    #modalNewSituationAlert.modal.fade(tabindex='-1', role='dialog', aria-labelledby='modalNewSituationAlert',
    aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title New Situation!
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')
          .modal-body
            p You could be affected by a new situation! Would you like to update your status?
          .modal-footer
            button.btn.btn-secondary(type='button', data-dismiss='modal', id='noUpdateStatusForSituation') NO
            button.btn.btn-primary(type='button', data-dismiss='modal', id='updateStatusForSituation') YES

    #modalRelevantSituations.modal.fade(tabindex='-1', role='dialog', aria-labelledby='modalOfflineSituationAlert',
    aria-hidden='true')
      .modal-dialog.short-modal(role='document')
        .modal-content
          .modal-header
            h5.modal-title#relevantSituationsTitle
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')
          .modal-body
            #listOfOfflineSituations
          .modal-footer
            button.btn.btn-primary#closeOfflineSituations(type='button', data-dismiss='modal') DISMISS

    #modalAffectedUserList.modal.fade(tabindex='-1', role='dialog', data-backdrop="static")
      .modal-dialog(role='document')
        .modal-content
          .modal-header
            h5.modal-title#affectedUserTitle Affected User List
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true') &times;
          .modal-body
            #affectedUsersGroup.list-group


